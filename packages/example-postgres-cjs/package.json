{
   "name": "example-postgres-cjs",
   "version": "1.0.0",
   "description": "",
   "type": "commonjs",
   "main": "./dist/index.js",
   "scripts": {
      "build": "tsc -b",
      "dev": "run-s 'build' 'dev:*'",
      "dev:1-start": "env-cmd --silent -x -r ../../env-dev.json -e db vite-node src/index.ts | pino-pretty",
      "codegen": "run-s 'codegen:*'",
      "codegen:1-generate": "env-cmd -x -r ../../env-dev.json -e db pg-typecraft generate --schema one_sql --pascalCaseTables --camelCaseColumns --host \\$POSTGRES_HOST --port \\$POSTGRES_PORT --database \\$POSTGRES_DATABASE --user \\$POSTGRES_USER --password \\$POSTGRES_PASSWORD --outDir 'src/codegen'",
      "postcodegen": "pnpx eslint ./src/codegen/**/*.ts --fix"
   },
   "keywords": [],
   "author": "",
   "license": "ISC",
   "dependencies": {
      "pg-typecraft": "workspace:^1.0.0",
      "postgres": "^3.4.7"
   }
}
